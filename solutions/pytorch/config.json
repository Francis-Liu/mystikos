{
    // OpenEnclave specific values

    // Whether we are running myst+OE+app in debug mode
    "Debug": 1,
    "ProductID": 1,
    "SecurityVersion": 1,

    // Mystikos specific values
    "EnvironmentVariables": [
        "LD_LIBRARY_PATH=/opt/conda/envs/pytorch-deps/lib"
